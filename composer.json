{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "lexik/jwt-authentication-bundle": "^2.15",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.4",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "captainhook/captainhook": "^5.10",
        "codeception/codeception": "^5.0",
        "codeception/module-doctrine2": "^3.0",
        "codeception/module-rest": "^3.0",
        "codeception/module-symfony": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "fzaninotto/faker": "^1.5",
        "symfony/maker-bundle": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-autoload-dump": [
            "vendor/bin/captainhook install -f -s"
        ],
        "lint": [
            "vendor/bin/phpcs -wap --colors"
        ],
        "lint-fix": [
            "vendor/bin/phpcbf -wvp"
        ],
        "test": [
            "vendor/bin/codecept run --steps"
        ],
        "test-generate-cest": [
            "vendor/bin/codecept generate:cest"
        ],
        "test-generate-cept": [
            "vendor/bin/codecept generate:cept"
        ],
        "test-generate-suite": [
            "vendor/bin/codecept generate:suite"
        ],
        "test-build": [
            "vendor/bin/codecept build"
        ],
        "database-scratch": [
            "bin/console doctrine:database:drop --force",
            "bin/console doctrine:database:create",
            "bin/console doctrine:migrations:migrate",
            "bin/console doctrine:fixtures:load"
        ],
        "database-test-scratch": [
            "bin/console --env=test doctrine:database:drop --force",
            "bin/console --env=test doctrine:database:create",
            "bin/console --env=test doctrine:migrations:migrate",
            "bin/console --env=test doctrine:fixtures:load"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
